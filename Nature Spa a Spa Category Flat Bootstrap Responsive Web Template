#add_payment_method #payment ul.payment_methods li .stripe-mastercard-brand, .woocommerce-checkout #payment ul.payment_methods li .stripe-mastercard-brand { position: absolute; top: 50%; margin-top: -10px; right: 10px; background: url("../images/mastercard.svg") no-repeat; display: block; width: 30px; height: 24px; }

